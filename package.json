{
  "name": "tutord-node-api-bp",
  "version": "1.0.0",
  "author": "Brandon Bailey",
  "license": "ISC",
  "description": "TutorD Node API Boilerplate for application development",
  "main": "src/server.js",
  "engines": {
    "node": ">= 8.11"
  },
  "scripts": {
    "start": "npm run build && nodemon --config nodemon.json --exec babel-node src/server.js",
    "start:debug": "npm run build && nodemon --inspect --config nodemon.json --exec babel-node src/server.js",
    "build": " npm run format && npm run clean:build && ./node_modules/.bin/babel src --out-dir ./dist --source-maps",
    "clean:all": "rimraf dist/ node_modules/",
    "clean:build": "rimraf dist/",
    "clean:node_modules": "rimraf node_modules/",
    "format": "./node_modules/.bin/prettier --config .prettierrc --write \"{src,seed,test}/**/*.js\"",
    "format:check": "./node_modules/.bin/prettier -l --config .prettierrc \"src/**/*.js\"",
    "test": "jest --watch",
    "test:ci": "jest && npm run format:check",
    "db:seed": "DEBUG=mongo-seeding babel-node seed/seed.js"
  },
  "keywords": [
    "TutorD",
    "Boilerplate",
    "bp"
  ],
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "bluebird": "3.7.2",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.22",
    "mongoose-deep-populate": "^3.2.0",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "randomstring": "^1.1.5",
    "serve-favicon": "^2.5.0",
    "util": "^0.12.3",
    "uuid": "^8.2.0",
    "whatwg-fetch": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.4",
    "babel-jest": "^26.1.0",
    "dotenv": "8.2.0",
    "https": "1.0.0",
    "jest": "^26.1.0",
    "mkdirp": "^1.0.4",
    "nodemon": "^2.0.4",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2"
  }
}
